Schema for the current version is below:




CREATE TABLE arrival (
	sta VARCHAR(6), 
	time FLOAT, 
	arid INTEGER NOT NULL, 
	jdate INTEGER, 
	stassid INTEGER, 
	chanid INTEGER, 
	chan VARCHAR(8), 
	iphase VARCHAR(8), 
	stype VARCHAR(1), 
	deltim FLOAT, 
	azimuth FLOAT, 
	delaz FLOAT, 
	slow FLOAT, 
	delslo FLOAT, 
	ema FLOAT, 
	rect FLOAT, 
	amp FLOAT, 
	per FLOAT, 
	logat FLOAT, 
	clip VARCHAR(1), 
	fm VARCHAR(2), 
	snr FLOAT, 
	qual VARCHAR(1), 
	auth VARCHAR(20), 
	commid INTEGER, 
	lddate DATETIME NOT NULL, 
	PRIMARY KEY (arid), 
	UNIQUE (sta, time, chan, iphase, auth)
);
CREATE TABLE affiliation (
	net VARCHAR(8) NOT NULL, 
	sta VARCHAR(6) NOT NULL, 
	time FLOAT NOT NULL, 
	endtime FLOAT, 
	lddate DATETIME NOT NULL, 
	PRIMARY KEY (net, sta, time)
);
CREATE TABLE assoc (
	arid INTEGER NOT NULL, 
	orid INTEGER NOT NULL, 
	sta VARCHAR(6), 
	phase VARCHAR(8), 
	belief FLOAT, 
	delta FLOAT, 
	seaz FLOAT, 
	esaz FLOAT, 
	timeres FLOAT, 
	timedef VARCHAR(1), 
	azres FLOAT, 
	azdef VARCHAR(1), 
	slores FLOAT, 
	slodef VARCHAR(1), 
	emares FLOAT, 
	wgt FLOAT, 
	vmodel VARCHAR(15), 
	commid INTEGER, 
	lddate DATETIME NOT NULL, 
	PRIMARY KEY (arid, orid), 
	UNIQUE (arid)
);
CREATE TABLE sitechan (
	sta VARCHAR(6), 
	chan VARCHAR(8), 
	ondate INTEGER, 
	chanid INTEGER NOT NULL, 
	offdate INTEGER, 
	ctype VARCHAR(4), 
	edepth FLOAT, 
	hang FLOAT, 
	vang FLOAT, 
	descrip VARCHAR(50), 
	lddate DATETIME NOT NULL, 
	PRIMARY KEY (chanid), 
	UNIQUE (sta, chan, ondate)
);
CREATE TABLE wfdisc (
	sta VARCHAR(6), 
	chan VARCHAR(8), 
	time FLOAT, 
	wfid INTEGER NOT NULL, 
	chanid INTEGER, 
	jdate INTEGER, 
	endtime FLOAT, 
	nsamp INTEGER, 
	samprate FLOAT, 
	calib FLOAT, 
	calper FLOAT, 
	instype VARCHAR(6), 
	segtype VARCHAR(1), 
	datatype VARCHAR(2), 
	clip VARCHAR(1), 
	dir VARCHAR(64), 
	dfile VARCHAR(32), 
	foff INTEGER, 
	commid INTEGER, 
	lddate DATETIME NOT NULL, 
	PRIMARY KEY (wfid), 
	UNIQUE (wfid, dir, dfile)
);
CREATE TABLE instrument (
	inid INTEGER NOT NULL, 
	insname VARCHAR(50), 
	instype VARCHAR(6), 
	band VARCHAR(1), 
	digital VARCHAR(1), 
	samprate FLOAT, 
	ncalib FLOAT, 
	ncalper FLOAT, 
	dir VARCHAR(64), 
	dfile VARCHAR(32), 
	rsptype VARCHAR(6), 
	lddate DATETIME NOT NULL, 
	PRIMARY KEY (inid)
);
CREATE TABLE lastid (
	keyname VARCHAR(15) NOT NULL, 
	keyvalue INTEGER, 
	lddate DATETIME NOT NULL, 
	PRIMARY KEY (keyname), 
	UNIQUE (keyname, keyvalue)
);
CREATE TABLE event (
	evid INTEGER NOT NULL, 
	evname VARCHAR(32), 
	prefor INTEGER, 
	auth VARCHAR(20), 
	commid INTEGER, 
	lddate DATETIME NOT NULL, 
	PRIMARY KEY (evid), 
	UNIQUE (prefor)
);
CREATE TABLE origin (
	lat FLOAT, 
	lon FLOAT, 
	depth FLOAT, 
	time FLOAT, 
	orid INTEGER NOT NULL, 
	evid INTEGER, 
	jdate INTEGER, 
	nass INTEGER, 
	ndef INTEGER, 
	ndp INTEGER, 
	grn INTEGER, 
	srn INTEGER, 
	etype VARCHAR(7), 
	depdp FLOAT, 
	dtype VARCHAR(1), 
	mb FLOAT, 
	mbid INTEGER, 
	ms FLOAT, 
	msid INTEGER, 
	ml FLOAT, 
	mlid INTEGER, 
	algorithm VARCHAR(15), 
	auth VARCHAR(20), 
	commid INTEGER, 
	lddate DATETIME NOT NULL, 
	PRIMARY KEY (orid), 
	UNIQUE (lat, lon, depth, time, auth)
);
CREATE TABLE site (
	sta VARCHAR(6) NOT NULL, 
	ondate INTEGER NOT NULL, 
	offdate INTEGER, 
	lat FLOAT, 
	lon FLOAT, 
	elev FLOAT, 
	staname VARCHAR(50), 
	statype VARCHAR(4), 
	refsta VARCHAR(6), 
	dnorth FLOAT, 
	deast FLOAT, 
	lddate DATETIME NOT NULL, 
	PRIMARY KEY (sta, ondate)
);
CREATE TABLE "FK_PARAMS" (
	pfkid INTEGER NOT NULL, 
	name VARCHAR(32), 
	filtertype VARCHAR(15), 
	filterl FLOAT, 
	filterh FLOAT, 
	fkfreql FLOAT, 
	fkfreqh FLOAT, 
	wlen FLOAT, 
	overlapwlen INTEGER, 
	minslowness FLOAT, 
	maxslowness FLOAT, 
	stepslowness FLOAT, 
	numsources INTEGER, 
	additional1 FLOAT, 
	additional2 FLOAT, 
	algorithm VARCHAR(15), 
	PRIMARY KEY (pfkid), 
	UNIQUE (overlapwlen, filterl, filterh, wlen, stepslowness, algorithm, fkfreql, fkfreqh, minslowness, maxslowness, numsources)
);
CREATE TABLE "Fk_results" (
	pfkid INTEGER, 
	fkid INTEGER NOT NULL, 
	chan VARCHAR(8), 
	nchan INTEGER, 
	sta VARCHAR(6), 
	timeini FLOAT, 
	timeend FLOAT, 
	sx FLOAT, 
	sy FLOAT, 
	esx FLOAT, 
	esy FLOAT, 
	bz FLOAT, 
	slofk FLOAT, 
	ebz FLOAT, 
	eslofk FLOAT, 
	fval FLOAT, 
	xcorrvalmax FLOAT, 
	xcorrvalmin FLOAT, 
	xcorrvalmean FLOAT, 
	rmsval FLOAT, 
	sourcenum INTEGER, 
	additional1 FLOAT, 
	additional2 FLOAT, 
	PRIMARY KEY (fkid)
);
CREATE TABLE "FD_PARAMS" (
	pfdid INTEGER NOT NULL, 
	name VARCHAR(32), 
	pthr FLOAT, 
	cthr FLOAT, 
	numsources INTEGER, 
	adapwlen FLOAT, 
	minlen FLOAT, 
	algorithm VARCHAR(15), 
	additional1 FLOAT, 
	additional2 FLOAT, 
	PRIMARY KEY (pfdid), 
	UNIQUE (pthr, cthr, adapwlen, minlen, algorithm, numsources)
);
CREATE TABLE "FD_RESULTS" (
	fdid INTEGER NOT NULL, 
	pfdid INTEGER, 
	pfkid INTEGER, 
	sta VARCHAR(6), 
	sourcenum INTEGER, 
	timeini FLOAT, 
	timeend FLOAT, 
	maxfc FLOAT, 
	maxpfc FLOAT, 
	maxfo FLOAT, 
	c FLOAT, 
	maxfc_time FLOAT, 
	etimeini FLOAT, 
	etimeend FLOAT, 
	emaxpfc FLOAT, 
	emaxfc FLOAT, 
	emaxfc_time FLOAT, 
	emaxfo FLOAT, 
	ec FLOAT, 
	fktablename VARCHAR(32), 
	PRIMARY KEY (fdid), 
	UNIQUE (fdid, pfdid, pfkid, fktablename)

